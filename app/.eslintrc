env:
  browser: true
  es6: true
  jquery: true
  mocha: true
  jasmine: true
  commonjs: true
globals:
  angular: true
extends: 'eslint:recommended'
parserOptions:
  sourceType: module
rules:
  indent:
    - error
    - 4
  linebreak-style:
    - error
    - unix
  quotes:
    - error
    - single
  semi:
    - error
    - always
