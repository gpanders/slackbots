    "mongoose": "^4.4.12",
    "ng-annotate-loader": "^0.1.0",
    "node-sass": "^3.5.3",
    "postcss-loader": "^0.8.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "concurrently": "^2.1.0",
    "dotenv": "^2.0.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
