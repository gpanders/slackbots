{
  "name": "slackbots-app",
  "private": true,
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "start": "node --harmony dist/index.js",
    "test": "concurrently \"npm run test:browser\" \"npm run test:server\"",
    "test:watch": "karma start karma.conf.js",
    "test:browser": "NODE_ENV=test karma start --single-run=true karma.conf.js",
    "test:server": "NODE_ENV=test mocha -R spec test",
    "postinstall": "npm run build",
    "build": "webpack"
  },
  "dependencies": {
    "@slack/client": "^3.1.0",
    "angular": "^1.5.5",
    "angular-storage": "0.0.15",
    "angular-ui-router": "^0.2.18",
    "angular-ui-sortable": "^0.14.0",
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "body-parser": "^1.15.0",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "ejs": "^2.4.1",
    "expose-loader": "^0.7.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.3",
    "jquery-ui": "^1.10.5",
